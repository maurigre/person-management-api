{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "data": {
      "type": "object",
      "properties": {
        "id": {
          "type": "string",
          "pattern": "[0-9a-fA-F]{8}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{12}"
        },
        "firstName": {
          "type": "string"
        },
        "lastName": {
          "type": "string"
        },
        "cpf": {
          "type": "string"
        },
        "birthDate": {
          "type": "string"
        },
        "phones": {
          "type": "array",
          "items": {
            "type": "object",
            "additionalProperties": true,
            "properties": {
              "type": {
                "enum": [
                  "HOME",
                  "MOBILE",
                  "COMMERCIAL"
                ]
              },
              "number": {
                "type": "string"
              }
            },
            "required" : ["type","number"]
          }
        }
      },
      "required" : ["firstName","lastName","cpf","birthDate","phones"]
    }
  },
  "required": [
    "data"
  ]
}